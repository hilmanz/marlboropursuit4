
<div class="theContent">
    <div class="theTitle">
        <h2>Summary User Journey</h2>
        <a href="index.php?s=axisuser&act=excel_listuser&search={$search}" class="btn btn_document"><span>Download XLS</span></a>
    </div><!--contenttitle-->
    <div class="tableoptions">
			<form>
            	<span>Search</span>
				<input type="hidden" name="s" value="reportactivityuser" />
				<input type="text" name="search" value="{$search}"/>
				{*}<span>Date Range</span>
				<input type="text" name="startdate" value="{$startdate}" class="datepicker"/>
				<span>s/d</span>	
				<input type="text" name="enddate" value="{$enddate}" class="datepicker"/>
				{/*}
				<input type="submit" value="cari" class="stdbtn btn_orange">
			</form>
    </div><!--tableoptions-->	
    <table cellpadding="0" cellspacing="0" border="0" id="table1" class="stdtable">
        <colgroup>
            <col class="con0" style="width: 4%" />
            <col class="con1" />
            <col class="con0" />
            <col class="con1" />
            <col class="con0" />
            <col class="con1" />
        </colgroup>
        <thead>
            <tr>
                <th class="head0">No</th>
                <th class="head1">Activity Name</th>
                <th class="head0">Action Value</th>
                <th class="head1">Total</th>
            </tr>
        </thead>
        <tbody>
            {section name=i loop=$list}
            <tr>
                <td width="3%">{$list[i].no}&nbsp;</td>
                <td>{$list[i].activityName}&nbsp;</td>
                <td>{$list[i].action_desc}&nbsp;</td>
                <td>{$list[i].total_value}&nbsp;</td>
            </tr>
            {/section}
        </tbody>
    </table>
    <div class="paging">
    	{$paging}
    </div>
</div><!--theContent-->