{$getData.title}
<div id="landing-page" class="section2">
	<div class="landing-page">
        <ul class="columns-content clearfix">
            <li class="col-sidebar2">
                <div class="sideBar">
                    <div class="entry">
                    	<h2>in making life decisions,<br />Only two options can move<br />
                        	you forward - Yes or No<br />
							<span class="red">Maybe</span> never can</h2>
                    </div><!-- END .entry -->
                    <div class="be-marlboro-small"></div>
                </div><!-- END .sideBar -->
            </li><!-- END .col-sidebar -->
            <li class="col-main2">
            	<div class="section-detail">
                    <ul class="columns-2 clearfix">
                        <li class="col2">
                        	<div class="widget-container news-widget">
                            	<div class="widget-title">
                                	<h3>Past Event</h3>
                                </div><!-- END .widget-title -->
                                <div class="widget-content">
                                	<ul class="tr-list">
                                    	<li class="row active">
                                            <h3 class="title"><a href="{$basedomain}mscape">What is MSCAPE?</a></h3>
                                        </li><!-- END .row -->
                                    	<li class="row">
                                            <h3 class="title"><a href="{$basedomain}mscape/photo">Photos</a></h3>
                                        </li><!-- END .row -->
                                    	<li class="row">
                                            <h3 class="title"><a href="{$basedomain}mscape/video">Videos</a></h3>
                                        </li><!-- END .row -->
                                    	<li class="row">
                                            <h3 class="title"><a href="{$basedomain}mscape/itinerary">2013 ITINERARY</a></h3>
                                        </li><!-- END .row -->
                                    </ul><!-- END .tr-list -->
                                </div><!-- END .widget-content -->
                            </div><!-- END .widget-container -->
                        </li><!-- END .col2 -->
                        <li class="col2">
                        	<div id="event-gallery">
                                <div class="perspective">
										<div class="perspective_diap">
										{foreach from=$getPhoto key=k item=v}
											<img src="{$basedomain}public_assets/mscape/{$v.image}" alt="image" />
											<p class="caption-event">
												<span class="entry-caption">
													<span class="title-photo">{$v.title}</span>
													<span class="date-event">{$v.posted_date}</span>
													<span class="venue-event">barangay</span>
												</span><!-- END .entry-caption -->
											</p><!-- END .caption -->
                                   {/foreach}
										</div><!-- END .perspective_diap -->
                                </div><!-- END .perspective -->
                                <div class="gallery-act">
                                    <div class="paging fr">
                                        <a class="prev" href="#">PREV</a>
                                        <a class="next" href="#">NEXT</a>
                                    </div><!-- END .paging -->
                                </div><!-- END .gallery-act -->
                            </div><!-- END #event-gallery -->
                        </li><!-- END .col2 -->
                    </ul><!-- END .columns-2 -->
                </div><!-- END .section-detail -->
            </li><!-- END .col-main -->
        </ul>
    </div><!-- END .landingPage -->
</div><!-- END #landingPage -->
{$popup_game}