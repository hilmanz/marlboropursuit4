                <h6>
                {if $v.articleType == 20}<a class="greyBtn showPopup" href="#popup-invitefriend" prop="{$v.id}">GO</a>{/if}				
				{*}{if $v.articleType == 18}<a class="greyBtn entercodetask"  href="javascript:void(0)" prop="{$v.id}">GO</a>{/if}{/*}
				{if $v.articleType == 21}<a class="greyBtn uploadphototask"  href="javascript:void(0)" prop="{$v.id}">GO</a>{/if}
				
				{if $v.articleType == 3}<a class="greyBtn gettask"  href="javascript:void(0)" prop="{$v.id}">GO</a>{/if}
				{if $v.articleType == 6}<a class="greyBtn gettask"  href="javascript:void(0)" prop="{$v.id}">GO</a>{/if}
