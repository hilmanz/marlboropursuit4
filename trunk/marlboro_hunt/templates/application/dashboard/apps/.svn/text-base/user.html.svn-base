<script type="text/javascript">


	var alluser = {$alluser.data|@json_encode};
	var loginData = {$loginData.data|@json_encode};
	var activeUser = {$activeUser.data|@json_encode};
	var veryActive = {$veryActive.data|@json_encode};
	var superUser = {$superUser.data|@json_encode};
	var userUnverified = {$userUnverified.data|@json_encode};
	
	//console.log(veryActive);
{literal}	
$(function () {

	var allUserData = new Array();
	var allUserDate = new Array();	
	
	var loginDataData = new Array();
	var loginDataDate = new Array();

	var actUserData = new Array();
	var actUserDate = new Array();
	
	var sprUserData = new Array();
	var sprUserDate = new Array();
	
	var vaUserData = new Array();
	var vaUserDate = new Array();
	
	var userUnverData = new Array();
	var userUnverDate = new Array();
		
    for (var dataDate in alluser){
			allUserData.push(parseInt(alluser[dataDate])); // coba['2013-01-01']['total']
			allUserDate.push(String(dataDate));
		}
		
    for (var logData in loginData){
			loginDataData.push(parseInt(loginData[logData])); // coba['2013-01-01']['total']
			loginDataDate.push(String(logData));
		}
		
	for (var actUsr in activeUser){
			actUserData.push(parseInt(activeUser[actUsr]));		
			actUserDate.push(String(actUsr));
		}
		
	for (var sprUsr in superUser){
			sprUserData.push(parseInt(superUser[sprUsr]));
			sprUserDate.push(String(sprUsr));
		}
	
	for (var vaData in veryActive){
			vaUserData.push(parseInt(veryActive[vaData]));
			vaUserDate.push(String(vaData));
		}
		
	for (var userUn in userUnverified){
			userUnverData.push(parseInt(userUnverified[userUn]));
			userUnverDate.push(String(userUn));
		}
		//console.log(vaData);
	
	gethighChart('column','test  Chart','allUserAct',allUserData,allUserDate,1,true);
	gethighChart('column','test  Chart','loginUser',loginDataData,loginDataDate,1,true);
	gethighChart('column','test  Chart','actUser',actUserData,actUserDate,1,true);
	gethighChart('column','test  Chart','veryActive',vaUserData,vaUserDate,1,true);
	gethighChart('column','test  Chart','sprUsrID',sprUserData,sprUserDate,1,true);
	gethighChart('column','test  Chart','unverifiedUser',userUnverData,userUnverDate,1,true);
	
	//drillDownChart('column','test Chart','cont',dataCoba,cateActivityGroup,1 ,false);
	});
		
</script>
