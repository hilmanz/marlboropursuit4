	<div class="box-letterTable last-box">
     	<div class="title_box">
            <div class="the-title fl">
                <h2>Number of redeem letters by channel</h2>
            </div>
         </div>
        <div class="letterTable-content">
        	<table class="gridtable">
			<tbody id="redeemLetters">
			 {foreach from=$numRedeemLetter key=k item=v}
			<tr>
				<td>{$v.histories}</td>
				<td>{$v.total}</td>
				
			</tr>
			 {/foreach}
             </tbody>
            </table>
            <div class="holder"></div>
        </div>
		
		
    </div><!--end.box-letterTable-->
	
	<div class="box-letterTable">
     	<div class="title_box">
