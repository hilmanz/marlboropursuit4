<script type="text/javascript">
	var numberRedeemLetter = {$numberRedeemLetter|@json_encode};
	
{literal}	
$(function () {

	var letterData = new Array();
	var letterDate = new Array();	
	
	for (var redeemLetter in numberRedeemLetter){
		letterData.push(parseInt(numberRedeemLetter[redeemLetter].num));
		letterDate.push(String(numberRedeemLetter[redeemLetter].codename));
	}
	
	gethighChart('column','test  Chart','letterDataField',letterData,letterDate,1,true);
	
	//drillDownChart('column','test Chart','cont',dataCoba,cateActivityGroup,1 ,false);
	});
	
	$(document).on('click','.calendar', function(){
		
		var from = $('#from').val();
		var to = $('#to').val();
		
		window.location.href='?startdate='+from+'&enddate='+to;
	
	})
		
</script>
{/literal}